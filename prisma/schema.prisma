generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Owner {
  id        String    @id
  name      String
  document  String    @unique
  createdAt DateTime
  updatedAt DateTime?

  @@map("owners")
}
